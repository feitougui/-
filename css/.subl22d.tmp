/* 初始化css */


/* 顶部 */
.top{
	background-color: #f5f5f5;
	height: 30px;
	width: 100%;
	position: relative;
}
.top ul{
	background-color: #f5f5f5;
	font-size: 24px;
	display: flex;
	position: absolute;
	right: 200px;
}
.top ul li{
	width: 60px;
	line-height: 30px;
	text-align: center;
	margin-left: 12px;
}
.top ul li a{
	background-color: #f5f5f5;
	display: block;
	width: 60px;
	height: 30px;
	text-align: center;
}
/* 登录窗口 */
#overDiv {	
	background-color:#000;	
	width: 100%;	
	height: 100%;	
	left: 0;	
	top: 0; 
	opacity: 0.65;  /* 透明属性 */
	z-index: 110;	
	position: fixed !important;
	position: absolute; 
	_top: expression(eval(document.compatMode && 	 document.compatMode == 'CSS1Compat')? 	documentElement.scrollTop+  (document.documentElement.clientHeight-this.offsetHeight		)/2: /*IE6*/ 	document.body.scrollTop+  (document.body.clientHeight-  this.clientHeight		)/2 );}
#login{
	background:#EDEDED;		
	z-index: 120;	
	width: 300px;	
	height: 200px;	
	left: 43%;
	top: 37%;	
	margin-top: 0px;	
	position: fixed !important; 
	position: absolute; 
	_top: expression(eval(document.compatMode && 	 document.compatMode == 'CSS1Compat')? 	documentElement.scrollTop+  (document.documentElement.clientHeight-this.offsetHeight		)/2: /*IE6*/ 	document.body.scrollTop+  (document.body.clientHeight-  this.clientHeight		)/2 ); 
}
#login #close_view{
	float: right;
}
#login #close_view a{
	background-color: #EDEDED;
	display: block;
	width: 100%;
	height: 100%;
}
#login .table{
	margin:0 auto;
	margin-top: 40px;
	width: 200px;
	height: 100px;
	background-color: #EDEDED;
}
#login .table .user{
	line-height: 30px;
	background-color: #EDEDED;
}
#login .table .password{
	line-height: 30px;
	margin-top: 20px;
	background-color: #EDEDED;
}
.rm_fg{
	text-align:right;
	background-color: #EDEDED;
}
.rm_fg a{
	font-size: 10px;
}
.log_reg{
	width: 100%;
	height: 46px;
	background-color: #EDEDED;
	text-align: center;
}
.log_reg input{
	margin-top: 15px;
}
.log_reg input:last-child{
	margin-left: 20px;
}
.containter{
	margin: 0 auto;
	width: 1190px;
	height: 1000px;
}
.containter .header{
	width: 100%;
	height: 200px;
	background:url(../images/school.jpg) no-repeat;
	background-size: 100% 100%;
}
.containter .tips{
	display: flex;
	width: 100%;
	height: 30px;
	margin-top: 5px;
	background-color: #EDEDED;
}
.containter .tips .tips_time{
	flex: 1;
	line-height: 30px;
}
.containter .tips .tips_container{
	flex: 5;
	line-height: 30px;
	position: relative;
}
.containter .tips .tips_container img{
	position: absolute;
	top: -15px;
	left: 120px;
}
/* 字体滚动 */
.containter .tips .tips_container .wrap{
	border-radius: 15px;
	position: relative;
	margin-left: 200px;
	width: 800px; 
	height: 30px; 
	overflow: hidden;
	white-space: nowrap;
}
.containter .tips .tips_container .wrap .cont{
	position: absolute;
 	height: 100%;
 	-webkit-animation:mymove 20s;
    animation-fill-mode: forwards;
}
.containter .tips .tips_container .wrap .txt{
	display: inline-block; 
  	font-size: 12px;
  	line-height: 30px; 
}
/* modules */
.modules{
	display: flex;
	width: 100%;
	height: 611px;
	margin-top: 5px;
}
/* moudules右侧 */
.modules_right{
	flex: 4;
	background-color: #174C4F;
}
.modules_right .search{
	width: 100%;
	line-height: 30px;
}
.modules_right .search .nav{
	width: 600px;
	height: 100%;
	float: right;
	right: 0px;
	top: 5px;
}
.modules_right .search .nav ul{
	display: flex;
}
.modules_right .search .nav ul li{
	flex: 1;
}
.modules_right .search .nav ul li:nth-child(-n+6){
	border-right: 1px solid #DEDEDE;
}
.modules_right .search .nav ul li a{
	display: block;
	width: 100%;
	height: 100%;
	font-size: 16px;
	text-align: center;
}
/* modules左侧 */
.modules_left{
	flex: 1;
	margin-left: 5px;
}
/* 系统公告 */
.modules_left .systemNotice{
	background-color: #f5f5f5;
	width: 100%;
	height: 200px;
	border: 1px solid #000;
}
.modules_left .systemNotice .systemNotice_title{
	color: #fff;
	line-height: 16px;
	font-size: 16px;
	background-color: #174C4F;
	border-bottom: 1px solid #B4B4B7;
	padding: 5px;
}
.modules_left .systemNotice .systemNotice_title img{
	padding-top: 3px;
}
.modules_left .systemNotice p{
	height: 144px;
	overflow: hidden;
	margin-top: 20px;
}
/* 校园资讯 */
.modules_left .schoolNews{
	background-color: #f5f5f5;
	width: 100%;
	height: 200px;
	margin-top: 5px;
	border: 1px solid #000;
}
.modules_left .schoolNews .schoolNews_title{
	color: #fff;
	line-height: 16px;
	background-color: #174C4F;
	font-size: 16px;
	border-bottom: 1px solid #B4B4B7;
	padding: 5px;
}
.modules_left .schoolNews .schoolNews_title img{
	padding-top: 3px;
}
.modules_left .schoolNews .newsItem{
	margin: 20px;
	width: 200px;
	height: 130px;
	
}
.modules_left .schoolNews .newsItem ul li{
	margin-top: 3px;
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.modules_left .yourSuggetion{
	background-color: #f5f5f5;
	border: 1px solid #000;
	margin-top: 5px;
	width: 100%;
	height: 200px;
}
.modules_left .yourSuggetion .yourSuggetion_title{
	color: #fff;
	line-height: 16px;
	font-size: 16px;
	background-color: #174C4F;
	border-bottom: 1px solid #B4B4B7;
	padding: 5px;
}
.modules_left .yourSuggetion .yourSuggetion_title img{
	padding-top: 3px;
}
.modules_left .yourSuggetion .yourSuggetion_content{
	position: relative;
	height: 153px;
	width: 203px;
	margin:20px;
}
.modules_left .yourSuggetion .yourSuggetion_content button{
	position: absolute;
	right: 2px;
	bottom: 5px;
}
